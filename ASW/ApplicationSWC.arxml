<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>SWComponent</SHORT-NAME>
			<ELEMENTS>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>TemControlSWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_Temperature</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/I_Temperature</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_StartMotor</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="CLIENT-SERVER-INTERFACE">/Interfaces/I_StartMotor</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_StopMotor</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="CLIENT-SERVER-INTERFACE">/Interfaces/I_StopMotor</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>IB_SWC</SHORT-NAME>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>TE_10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWComponent/TemControlSWC/IB_SWC/TemperatureControl</START-ON-EVENT-REF>
									<PERIOD>0.01</PERIOD>
								</TIMING-EVENT>
							</EVENTS>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>TemperatureControl</SHORT-NAME>
									<DATA-RECEIVE-POINT-BY-ARGUMENTS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>VA_Temp</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/SWComponent/TemControlSWC/RP_Temperature</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/I_Temperature/Temperature</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-RECEIVE-POINT-BY-ARGUMENTS>
									<SERVER-CALL-POINTS>
										<SYNCHRONOUS-SERVER-CALL-POINT>
											<SHORT-NAME>SC_StartMotor</SHORT-NAME>
											<OPERATION-IREF>
												<CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWComponent/TemControlSWC/RP_StartMotor</CONTEXT-R-PORT-REF>
												<TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/I_StartMotor/StartMotor</TARGET-REQUIRED-OPERATION-REF>
											</OPERATION-IREF>
										</SYNCHRONOUS-SERVER-CALL-POINT>
										<SYNCHRONOUS-SERVER-CALL-POINT>
											<SHORT-NAME>SC_StopMotor</SHORT-NAME>
											<OPERATION-IREF>
												<CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWComponent/TemControlSWC/RP_StopMotor</CONTEXT-R-PORT-REF>
												<TARGET-REQUIRED-OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/I_StopMotor/StopMotor</TARGET-REQUIRED-OPERATION-REF>
											</OPERATION-IREF>
										</SYNCHRONOUS-SERVER-CALL-POINT>
									</SERVER-CALL-POINTS>
									<SYMBOL>ApplTemperatureControl</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>
